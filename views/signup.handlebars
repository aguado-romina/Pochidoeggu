            <label for="exampleInputPassword1" class="form-label" for="SignUpPassword">Password</label>
            <!--Kept
            for="LoginPassword"-->
            <input type="password" class="form-control" id="exampleInputPassword1" type="text" id="SignUpPassword"
                name="name">
            <!--Kept type="text" id="SignUpPassword" name="name"--->
        </div>
        <button type="submit" class="btn btn-primary" type="submit" id="btnColor">Create Account</button>
        <!--Kept type="submit"--->
    </form>

